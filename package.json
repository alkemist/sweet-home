{
  "name": "sweet-home",
  "version": "2.1.6",
  "scripts": {
    "ng": "ng",
    "dev": "npm run config -- --environment=dev && ng serve --port=4201",
    "start": "npm run config -- --environment=dev && ng serve --ssl true --ssl-key ~/key.pem --ssl-cert ~/cert.pem",
    "serve": "npm run start -- --no-live-reload",
    "start-live": "npm run start -- --host 0.0.0.0 --disable-host-check",
    "serve-live": "npm run serve -- --host 0.0.0.0 --disable-host-check",
    "build-worker": "rm -rf src/assets/workers && tsc-esm --project tsconfig.worker.json",
    "build": "npm run build-worker ; npm run config -- --environment=prod ; ng build  --configuration production --base-href /",
    "deploy": "git pull; yarn install; rm -rf wwww ; yarn build",
    "build-watch": "npm run build -- --watch",
    "watch": "ng build --configuration development --watch",
    "test": "jest",
    "reset": "rm -rf node_modules && rm yarn.lock && yarn install",
    "config": "ts-node ./scripts/setenv.ts",
    "rebuild": "cordova platform rm android && cordova platform add android && cordova plugin remove cordova-plugin-add-swift-support --force",
    "prepare": "ts-patch install -s"
  },
  "private": true,
  "dependencies": {
    "@alkemist/compare-engine": "^1.3.6",
    "@alkemist/ngx-data-store": "^1.1.38",
    "@alkemist/ngx-form-supervisor": "^1.1.16",
    "@alkemist/ngx-state-manager": "^1.2.27",
    "@alkemist/smart-tools": "^1.1.20",
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "^16.2.0",
    "@digitak/tsc-esm": "^3.1.4",
    "@ngx-translate/core": "^15.0.0",
    "chart.js": "^4.4.0",
    "crossoriginworker": "^1.1.0",
    "dateformat": "^5.0.3",
    "dotenv": "^16.0.3",
    "fork-awesome": "^1.2.0",
    "hammerjs": "^2.0.8",
    "json-rpc-2.0": "^1.5.1",
    "ngx-valdemort": "^10.0.1",
    "nosleep.js": "^0.12.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "16.7.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "yarn": "^1.22.19",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "16.2.0",
    "@angular/compiler-cli": "^16.2.2",
    "@angular/localize": "^16.2.2",
    "@types/chart.js": "^2.9.40",
    "@types/cordova": "^11.0.0",
    "@types/dateformat": "^5.0.2",
    "@types/hammerjs": "^2.0.41",
    "@types/jest": "^27.4.0",
    "@types/node": "^20.2.3",
    "@types/xml2js": "^0.4.11",
    "cordova-android": "^10.1.2",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-app-event": "^1.2.2",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-geofence": "^0.7.0",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-network-information": "^3.0.0",
    "es6-promise-plugin": "^4.2.2",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-preset-angular": "^13.1.1",
    "ng-mocks": "^14.10.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.5.0",
    "ts-patch": "^2.1.0",
    "typescript": "5.0",
    "typescript-transform-extensions": "^1.0.1",
    "webpack": "^5.76.2"
  },
  "cordova": {
    "platforms": [],
    "plugins": {
      "cordova-plugin-androidx-adapter": {},
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      },
      "cordova-plugin-network-information": {},
      "cordova-plugin-geofence": {}
    }
  }
}
