{
  "name": "sweet-home",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "serve": "ng serve --host 0.0.0.0 --disable-host-check --no-live-reload",
    "build": "ng build  --configuration production --base-href /",
    "build-watch": "ng build  --configuration production --base-href / --watch",
    "watch": "ng build --configuration development --watch",
    "test": "ng test",
    "reset": "rm -rf node_modules && rm yarn.lock && yarn install",
    "netlify": "netlify dev",
    "rebuild": "cordova platform rm android && cordova platform add android && cordova plugin remove cordova-plugin-add-swift-support --force",
    "server": "sudo http-server dist/sweet-home --https --port 443 -S -C ~/cert.pem -K ~/key.pem -o --cors.origin https://dev.my-sweet-home.netlify.app/ --cors.credentials"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
    "@angular/core": "^15.2.7",
    "@angular/forms": "^15.2.7",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@angular/router": "^15.2.7",
    "@angular/service-worker": "^15.2.7",
    "@ngxs/logger-plugin": "^3.8.0",
    "@ngxs/storage-plugin": "^3.8.0",
    "@ngxs/store": "^3.8.0",
    "add": "^2.0.6",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.3",
    "firebase": "^9.17.1",
    "fork-awesome": "^1.2.0",
    "hammerjs": "^2.0.8",
    "json-rpc-2.0": "^1.5.1",
    "ngx-valdemort": "^9.0.0",
    "nosleep.js": "^0.12.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "^15.2.0",
    "rxjs": "~7.8.0",
    "stackdriver-errors-js": "^0.12.0",
    "stream": "^0.0.2",
    "timers-browserify": "^2.0.12",
    "tslib": "^2.3.0",
    "xml2js": "^0.4.23",
    "yarn": "^1.22.19",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "~15.2.6",
    "@angular/compiler-cli": "^15.2.7",
    "@angular/localize": "^15.2.7",
    "@netlify/functions": "^1.0.0",
    "@ngxs/devtools-plugin": "^3.8.0",
    "@types/cordova": "^11.0.0",
    "@types/hammerjs": "^2.0.41",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.14.0",
    "@types/xml2js": "^0.4.11",
    "cordova-android": "^10.1.2",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-app-event": "^1.2.2",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-geofence": "github:tabrindle/Geofencing",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-network-information": "^3.0.0",
    "es6-promise-plugin": "^4.2.2",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-preset-angular": "^11.1.1",
    "netlify-cli": "^9.13.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "~4.9.4",
    "webpack": "^5.76.2"
  },
  "cordova": {
    "platforms": [],
    "plugins": {
      "cordova-plugin-androidx-adapter": {},
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      },
      "cordova-plugin-network-information": {},
      "cordova-plugin-geofence": {}
    }
  }
}
