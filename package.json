{
  "name": "sweet-home",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "serve": "ng serve --host 0.0.0.0 --disable-host-check --no-live-reload",
    "build": "ng build --prod --base-href ./",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "reset": "rm -rf node_modules && rm yarn.lock && yarn install",
    "server": "sudo http-server dist/sweet-home --https --port 443 -S -C ~/cert.pem -K ~/key.pem -o --cors.origin https://dev.my-sweet-home.netlify.app/ --cors.credentials"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@angular/service-worker": "^15.1.0",
    "@ngxs/logger-plugin": "^3.8.0",
    "@ngxs/storage-plugin": "^3.8.0",
    "@ngxs/store": "^3.8.0",
    "add": "^2.0.6",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.3",
    "firebase": "^9.17.1",
    "fork-awesome": "^1.2.0",
    "hammerjs": "^2.0.8",
    "json-rpc-2.0": "^1.5.1",
    "ngx-valdemort": "^9.0.0",
    "nosleep.js": "^0.12.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "^15.2.0",
    "rxjs": "~7.8.0",
    "stackdriver-errors-js": "^0.12.0",
    "tslib": "^2.3.0",
    "yarn": "^1.22.19",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular/cli": "~15.1.6",
    "@angular/compiler-cli": "^15.1.0",
    "@angular/localize": "^15.1.5",
    "@netlify/functions": "^1.0.0",
    "@ngxs/devtools-plugin": "^3.8.0",
    "@types/hammerjs": "^2.0.41",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.14.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-preset-angular": "^11.1.1",
    "netlify-cli": "^9.13.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "~4.9.4",
    "webpack": "^5.76.2"
  }
}
