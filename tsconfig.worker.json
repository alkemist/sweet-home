/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./src/assets/workers",
    "lib": [
      "es2018",
      "webworker"
    ],
    "types": [],
    "plugins": [
      {
        "transform": "typescript-transform-extensions",
        "extensions": {".js": ".js"}
      }
    ]
  },
  "include": [
    "src/workers/*.ts"
  ],
  "exclude": [
    "./out-tsc/app"
  ]
}
